          "queryType": "sql",
          "rawSql": "SELECT\n  toDateTime(intDiv(toUInt32(event_time), 300) * 300) AS time,\n  event_presentation AS event,\n  count() AS count\nFROM logs.event_log\nWHERE $__timeFilter(event_time)\n  AND event_presentation IN ('Данные.Изменение', 'Данные.Создание', 'Данные.Удаление')\n  AND metadata_presentation != ''\n  AND ('$__all' IN ($infobase) OR metadata_presentation IN ($infobase))\nGROUP BY time, event\nORDER BY time",
          "refId": "A"
        }
